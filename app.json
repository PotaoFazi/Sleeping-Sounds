{
  "expo": {
    "name": "Soothe Station",
    "slug": "Sleeping-sounds",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/StartIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/AppIcon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/StartIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.talha_farrukh.SOOTHESTATION"
    },
    "web": {
      "favicon": "./assets/StartIcon.png"
    }
  }
}
